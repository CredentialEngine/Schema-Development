@prefix ceterms: <https://purl.org/ctdl/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .

ceterms:Constraint a rdf:Class .

ceterms:hasConstraint a rdf:Property ;
	schema:rangeIncludes ceterms:Constraint .

ceterms:leftOperand  a rdf:Property ;
	schema:rangeIncludes rdf:Literal .

ceterms:operand  a rdf:Property ;
	schema:rangeIncludes rdf:Literal .

ceterms:rightOperand  a rdf:Property ;
	schema:rangeIncludes rdf:Literal .
