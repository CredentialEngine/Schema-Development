			"skos:prefLabel": { "en": "Knowledge" },
			"skos:definition": { "en": "In the context of the EQF, knowledge is described as theoretical and/or factual." },
			"skos:inScheme": "res:facets",
			"skos:topConceptOf": "res:facets"
		},
